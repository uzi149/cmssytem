@model cmsbeta5.Models.Login
@using cmsbeta5.Models
@{
    ViewBag.Title = "listOfFreezeAccounts";
}
<body>
    <nav class="navbar navbar-default" style="background-color:#F9864D; color:white;">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" style="background-color:#F9864D; color:white;" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only" style="background-color:#F9864D; color:white;">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#" style="background-color:#F9864D; color:white;">Fake College</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">



                    <li><a href="AddTeacher"><label style="background-color:#F9864D; color:white;">Add Teacher</label> </a></li>
                    <li><a href="AddStudent"><label style="background-color:#F9864D; color:white;">Add Student</label> </a></li>
                    <li><a href="ResetPassword"><label style="background-color:#F9864D; color:white;">Reset Password</label> </a></li>
                    <li><a href="FreezeMember"><label style="background-color:#F9864D; color:white;">Freeze Account</label> </a></li>
                    <li><a href="listOfFreezeAccounts"><label style="background-color:#F9864D; color:white;">Freeze Accounts</label> </a></li>




                </ul>

                <ul class="nav navbar-nav navbar-right">
                    <div class="dropdown">
                        <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true">
                            Admin
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="/Main " style="background-color:#F9864D; color:white;">Logout</a></li>
                            <li role="presentation"><a role="menuitem" tabindex="-1" href="/Admin/changePassword" style="background-color:#F9864D; color:white;">Change Password </a></li>

                        </ul>
                    </div>

                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
    <table class="table table-bordered table-responsive" style="width:200px;margin-left:480px;color:green;">

        <tr>
            <th>
                List of All the Freeze Accounts
            </th>
            <th>
                Action
            </th>
        </tr>

        @foreach (Login s in ViewBag.list)
        {

            <tr>
                <td>
                    @s.Id
                </td>
             
                <td>
                    @Html.ActionLink("Unfreeze", "unfreezeAccount", new { loginId = @s.Id }, new { @style = "color:white;", @class = "btn" })
                    </td>
               
            </tr>


        }
    </table>
</body>

